<!DOCTYPE html>
<html>
    <head>
        <title>admin adhesions page</title>
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link type="text/css" rel="stylesheet" href="/stylesheets/materialize.min.css"  media="screen,projection"/>

        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    </head>
    <body>

        <table class="centered">
            <thead>
                <tr>
                    <th>prénom</th>
                    <th>nom</th>
                    <th>service</th>
                    <th>email</th>
                    <th>téléphone</th>
                    <th>Supprimer</th>
                </tr>
            </thead>

            <tbody>

                <% adhesions.forEach(function(adhesion){ %>
                <tr>
                    <td><%= adhesion.firstname %></td>
                    <td><%= adhesion.lastname %></td>
                    <td><%= adhesion.service %></td>
                    <td><%= adhesion.email %></td>
                    <td><%= adhesion.telephone %></td>
                    <td>
                        <a class="delete-adhesion btn-floating btn-small waves-effect waves-light red">
                            <input type="hidden" value="<%= adhesion.id %>" />
                            <i class="material-icons">clear</i>
                        </a>
                    </td>
                </tr>
                <%} ); %>
            </tbody>
        </table>


        <div class="fixed-action-btn">
            <a href="/admin/gestion/" class="btn-floating btn-large blue lighten-3">
                <i class="large material-icons">keyboard_backspace</i>
            </a>
        </div>

        <script type="text/javascript" src="/javascripts/jquery.js"></script>
        <script type="text/javascript" src="/javascripts/materialize.min.js"></script>
        <script type="text/javascript" src="/javascripts/sweetalert.js"></script>
        <script type="text/javascript" src="/javascripts/imports.js"></script>
        <script type="text/javascript" src="/javascripts/admin-actions.js"></script>
    </body>
</html>